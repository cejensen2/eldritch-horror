{
  "name": "eldritch-horror",
  "version": "1.0.0",
  "description": "A basic recreation of my favorite board game",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "webpack -w & nodemon server.js",
    "start-electron": "npm run start & electron .",
    "preelectron-pack": "npm run build",
