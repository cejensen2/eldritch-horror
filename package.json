    "babel-loader": "^8.0.0-beta.4",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.5",
    "electron-is-dev": "^1.0.1",
    "eslint": "^4.19.1",
    "eslint-config-fullstack": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-react": "^7.12.4",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "react-scripts": "^2.1.3"
